    "from lxml import html\n",
    "from urllib.request import Request, urlopen\n",
    "import pandas as pd\n",
    "\n",
    "# URL a acceder\n",
    "url = 'https://www.imdb.com/search/title/?genres=comedy&languages=es'\n",
    "\n",
    "# Crear una solicitud con un User-Agent\n",
    "req = Request(url, headers={'User-Agent' : 'Magic Browser', 'Accept-Language': 'es-ES'})\n",
    "response = urlopen(req)\n",
    "source = response.read()\n",
    "\n",
    "# Construir la estructura de árbol\n",
    "tree = html.fromstring(source)\n",
    "\n",
    "# Encontrar todos los contenedores de elementos de lista\n",
    "resultado_peli = tree.cssselect(\"li.ipc-metadata-list-summary-item\")\n",
    "print(f'Número de elementos encontrados: {len(resultado_peli)}')\n",
    "\n",
    "peliculas = []\n",
    "\n",
    "for pelicula in resultado_peli:\n",
    "    # Búsqueda de los elementos de cada película\n",
    "    titulo_tag = pelicula.cssselect(\"h3.ipc-title__text\")\n",
    "    point_tag = pelicula.cssselect(\"span.ratingGroup--imdb-rating\")\n",
    "    #votos_tag = pelicula.cssselect(\"span.ipc-rating-star--voteCount\")\n",
    "\n",
    "    titulo = titulo_tag[0].text_content() if titulo_tag else 'N/A'\n",
    "    ratingPoint = point_tag[0].text_content() if point_tag else 'N/A'\n",
    "    #votos = votos_tag[0].text_content() if votos_tag else 'N/A'\n",
    "\n",
    "\n",
    "    # Construye diccionario de una película\n",
    "    pelicula_dict = {\n",
    "        'Nombre': titulo,\n",
    "        'Votos': ratingPoint,\n",
    "       # 'Votos': votos,\n",
    "    }\n",
    "     \n",
    "    # Utiliza append para acumular las películas\n",
    "    peliculas.append(pelicula_dict)\n",
    "\n",
    "# Convierte el listado de diccionarios en un DataFrame de pandas\n",
    "df_peliculas = pd.DataFrame(peliculas)\n",
    "\n",
    "# Extraer la puntuación de la columna 'Votos' y crear una nueva columna 'Puntuación'\n",
    "df_peliculas['Puntuacion'] = df_peliculas['Votos'].apply(lambda x: x.split()[0])\n",
    "df_peliculas['Votos'] = df_peliculas['Votos'].apply(lambda x: x.split()[1] if len(x.split()) > 1 else '')\n",
    "df_peliculas['Votos'] = df_peliculas['Votos'].str.replace(r'[^\d,]', '', regex=True)\n",
    "\n",
    "# Ordenar las columnas para mostrar primero 'Nombre', 'Puntuación' y 'Votos'\n",
    "df_peliculas = df_peliculas[['Nombre', 'Puntuacion', 'Votos']]\n",
    "\n",
    "# Imprimir las primeras 25 filas del DataFrame sin índice\n",
    "print(df_peliculas.head(25).to_string(index=False))\n",
    "\n"
   ]
